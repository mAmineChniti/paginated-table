{
  "name": "paginated-table",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "prettier": "prettier \"**/*.{js,mjs,ts,tsx,md,mdx,json,yml,css}\" --check --cache --cache-strategy=content --cache-location=.prettiercache",
    "prettier:fix": "prettier --write .",
    "format": "bun run lint:fix && bun run prettier:fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "next lint --fix",
    "**/*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-table": "^8.19.2",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "geist": "^1.3.0",
    "next": "^14.2.1",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.35.0"
  }
}
